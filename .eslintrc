{
  "extends": ["airbnb-base"],
  "env": {
    "browser": true
  },
  "rules": {
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "no-param-reassign": "off",
    "prefer-rest-params": "off",
    "max-len": ["error", {"code": 120, "comments": 100}],
    "no-multi-assign": "off",
    "import/prefer-default-export": "off"
  }
}
